{
  "dashboard": {
    "id": null,
    "title": "EV Bunker Application Dashboard",
    "tags": ["ev-bunker", "nextjs", "mongodb", "redis"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "HTTP Requests Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "HTTP Request Duration",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Seconds"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "singlestat",
        "title": "Current Error Rate",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])",
            "refId": "A"
          }
        ],
        "format": "percentunit",
        "prefix": "Error Rate: ",
        "postfix": "%",
        "thresholds": "5,10",
        "colorBackground": true,
        "colors": ["#299c46", "rgba(237, 129, 40, 0.89)", "#d44a3a"]
      },
      {
        "id": 4,
        "type": "singlestat",
        "title": "Average Response Time",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "avg(http_request_duration_seconds_sum) / avg(http_request_duration_seconds_count)",
            "refId": "A"
          }
        ],
        "format": "s",
        "prefix": "Avg: ",
        "thresholds": "0.2,0.5",
        "colorBackground": false,
        "colors": ["#299c46", "rgba(237, 129, 40, 0.89)", "#d44a3a"]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Database Connections",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "mongodb_connections{state=\"current\"}",
            "legendFormat": "Current Connections",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections{state=\"available\"}",
            "legendFormat": "Available Connections",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Redis Memory Usage",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Max Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    }
  }
}